{
  "name": "@dicebear/micah",
  "version": "4.7.0-rc.0",
  "description": "Micah avatar style for DiceBear Avatars",
  "keywords": [
    "dicebear",
    "avatar",
    "micah",
    "svg"
  ],
  "homepage": "https://dicebear.com",
  "bugs": {
    "url": "https://github.com/dicebear/dicebear/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicebear/dicebear.git"
  },
  "license": "MIT",
  "author": "Florian KÃ¶rner <contact@florian-koerner.com>",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "prebuild": "shx rm -rf dist",
    "build": "yarn run build:schema && yarn run build:options && yarn run build:js",
    "build:js": "tsc --emitDeclarationOnly && rollup -c",
    "build:schema": "yarn run build:schema:dereference && yarn run build:schema:module",
    "build:schema:dereference": "json-dereference -s schema.json -o src/schema.json",
    "build:schema:module": "replace-in-file --configFile=config/schema.replace.js && shx mv src/schema.json src/schema.ts",
    "build:options": "json2ts -i schema.json -o src/options.ts"
  },
  "devDependencies": {
    "@dicebear/avatars": "^4.7.0-rc.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.22",
    "@types/node": "^10.11.6",
    "jest": "^26.6.3",
    "json-dereference-cli": "^0.1.2",
    "replace-in-file": "^6.2.0",
    "rollup": "^2.45.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "utility-types": "^3.10.0"
  },
  "peerDependencies": {
    "@dicebear/avatars": "^4.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
