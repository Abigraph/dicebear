{
  "title": "Options",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "eyeColor": {
      "$ref": "#/properties/eyeShadowColor",
      "description": "@deprecated use eyeShadow instead."
    },
    "base": {
      "title": "Base",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "standard"
        ]
      },
      "default": [
        "standard"
      ]
    },
    "mouth": {
      "title": "Mouth",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "surprised",
          "laughing",
          "nervous",
          "smile",
          "sad",
          "pucker",
          "frown",
          "smirk"
        ]
      },
      "default": [
        "surprised",
        "laughing",
        "nervous",
        "smile",
        "sad",
        "pucker",
        "frown",
        "smirk"
      ]
    },
    "eyebrows": {
      "title": "Eyebrows",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "up",
          "down",
          "eyelashesUp",
          "eyelashesDown"
        ]
      },
      "default": [
        "up",
        "down",
        "eyelashesUp",
        "eyelashesDown"
      ]
    },
    "hair": {
      "title": "Hair",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "fonze",
          "mrT",
          "dougFunny",
          "mrClean",
          "dannyPhantom",
          "full",
          "turban",
          "pixie"
        ]
      },
      "default": [
        "fonze",
        "mrT",
        "dougFunny",
        "mrClean",
        "dannyPhantom",
        "full",
        "turban",
        "pixie"
      ]
    },
    "hairProbability": {
      "title": "Hair Probability",
      "$ref": "https://dicebear.com/schema/v4.json#/definitions/probability",
      "default": 100
    },
    "eyes": {
      "title": "Eyes",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "eyes",
          "round",
          "eyesShadow",
          "smiling"
        ]
      },
      "default": [
        "eyes",
        "round",
        "eyesShadow",
        "smiling"
      ]
    },
    "nose": {
      "title": "Nose",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "curve",
          "pointed",
          "tound"
        ]
      },
      "default": [
        "curve",
        "pointed",
        "tound"
      ]
    },
    "ears": {
      "title": "Ears",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "attached",
          "detached"
        ]
      },
      "default": [
        "attached",
        "detached"
      ]
    },
    "shirt": {
      "title": "Shirt",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "open",
          "crew",
          "collared"
        ]
      },
      "default": [
        "open",
        "crew",
        "collared"
      ]
    },
    "earrings": {
      "title": "Earrings",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "hoop",
          "stud"
        ]
      },
      "default": [
        "hoop",
        "stud"
      ]
    },
    "earringsProbability": {
      "title": "Earrings Probability",
      "$ref": "https://dicebear.com/schema/v4.json#/definitions/probability",
      "default": 30
    },
    "glasses": {
      "title": "Glasses",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "round",
          "square"
        ]
      },
      "default": [
        "round",
        "square"
      ]
    },
    "glassesProbability": {
      "title": "Glasses Probability",
      "$ref": "https://dicebear.com/schema/v4.json#/definitions/probability",
      "default": 30
    },
    "facialHair": {
      "title": "Facial Hair",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "beard",
          "scruff"
        ]
      },
      "default": [
        "beard",
        "scruff"
      ]
    },
    "facialHairProbability": {
      "title": "Facial Hair Probability",
      "$ref": "https://dicebear.com/schema/v4.json#/definitions/probability",
      "default": 10
    },
    "baseColor": {
      "title": "Base Color",
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string",
            "enum": [
              "apricot",
              "coast",
              "topaz",
              "lavender",
              "sky",
              "salmon",
              "canary",
              "calm",
              "azure",
              "seashell",
              "mellow",
              "black",
              "white"
            ]
          },
          {
            "$ref": "https://dicebear.com/schema/v4.json#/definitions/color"
          }
        ]
      },
      "default": [
        "apricot",
        "coast",
        "topaz"
      ]
    },
    "earringColor": {
      "title": "Earring Color",
      "type": "array",
      "items": {
