      "default": ["open", "sleep", "wink", "glasses", "happy", "sunglasses"],
      "examples": [
        ["open"],
        ["sleep"],
        ["wink"],
        ["glasses"],
        ["happy"],
        ["sunglasses"]
      ]
    },
    "facialHair": {
