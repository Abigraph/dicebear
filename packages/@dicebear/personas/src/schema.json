      },
      "default": [
        "beardMustache",
        "pyramid",
        "walrus",
        "goatee",
        "shadow",
        "soulPatch"
      ],
      "examples": [
        ["beardMustache"],
        ["pyramid"],
        ["walrus"],
        ["goatee"],
        ["shadow"],
        ["soulPatch"]
      ]
    },
    "hair": {
